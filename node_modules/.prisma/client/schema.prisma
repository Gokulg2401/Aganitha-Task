generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Link {
  id            Int       @id @default(autoincrement())
  code          String    @unique
  originalUrl   String
  clicks        Int       @default(0)
  lastClickedAt DateTime?
  createdAt     DateTime  @default(now())
}
